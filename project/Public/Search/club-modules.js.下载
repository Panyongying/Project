//		});
//	};
	
	this.checkIfValidForBooking = function(){
		
		var valid = scope.voucherForm && scope.voucherForm.$valid;
		
		if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_EVENT){	
			valid = !!(valid && scope.selectedVenue);
		}
		else if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE){
			valid = !!(valid && scope.selectedStore && scope.selectedDate && scope.selectedTimeSlot);
		}
		
		if(scope.isEventOnlyOneSeatLeft && scope.preferences.bringAFriendChecked){
			valid = false;
		}
		return valid;
	};
	
	this.buildManageBookingPayload = function(bookingId){
		
		var payload = {
				venueCompanyId: scope.venueCompanyId,				
				email: scope.clubModel.personalDetails.email,
				firstName: scope.clubModel.personalDetails.firstName,
				lastName: scope.clubModel.personalDetails.lastName,				
				notificationSms: scope.preferences.smsAlertRequired,
				notificationEmail: scope.preferences.mailAlertRequired,
				offerKey: scope.offerKey,
				offerPropositionId: scope.offerPropositionId,
				customerLoyaltyId: scope.customerLoyaltyId,				
				languageCode: $rootScope.language,
				contentUrl: window.location.href.split('.html')[0] + '.bb.xml'			
		};
		
		if(bookingId){
			payload.bookingId = bookingId;
			payload.points = 0;
		}else{
			payload.points = scope.pointsRedeemCost;
		}
		
		if(scope.clubModel.personalDetails.phonePrefix && scope.clubModel.personalDetails.phoneNumber){
			payload.phoneNumber = scope.clubModel.personalDetails.phonePrefix + " " + scope.clubModel.personalDetails.phoneNumber;
		}else{
			payload.phoneNumber = "";
		}
		scope.clubModel.personalDetails.name = payload.firstName + " " + payload.lastName;
		scope.clubModel.personalDetails.fullPhoneNumber = payload.phoneNumber;
		
		if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_EVENT){
			payload.eventInfoVenueEventId = scope.selectedVenue.venueEventId;
			payload.eventInfoVenueEventChainId = scope.selectedVenue.venueEventChainId;
			payload.eventInfoBringFriends = (scope.preferences.bringAFriendChecked ? 1 : 0);
			payload.eventInfoFriendsNames = scope.preferences.bringAFriendName;
			payload.bookedDateTime = scope.selectedVenue.eventDateTime;
			payload.duration = scope.selectedVenue.duration;
		}
		else if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE){
			payload.serviceInfoVenueServiceId = scope.selectedStore.venueServiceId;
			payload.bookedDateTime = scope.selectedDate.date + "T" + scope.selectedTimeSlot.time + ":00+"+sessionStorage.getItem(scope.timeShiftParam);	//this is orrid!
			payload.duration = scope.selectedTimeSlot.duration;
		}
		
		return payload;
	};
	
	this.updateStatusAfterManageBooking = function(){		
		this.loadBookingInfo(scope.bookingId, scope.venueCompanyId);
	};
	
	this.setAddToCalendarUrls = function(){

		//NOTE: we are currently missing all localization informations, so the times could be inaccurate
		
		var date =  (scope.whenDate + "T" + scope.whenTime).replace(/[\s-:]/g,'') + "00",
			where = (scope.whereStreet + " " + scope.whereCity).replace('&','');
		
		var endDate = moment(date, "YYYYMMDD[T]HHmmss").add(1,'hours').format("YYYYMMDD[T]HHmmss");

		var details_ics = scope.whatLabelLocalized+"\\n"
					 +scope.voucherWhat+"\\n\\n"
					 +scope.whereLabelLocalized+"\\n"
					 +where+"\\n\\n"
					 +scope.whenLabelLocalized+"\\n"
					 +scope.whenDate+" "+scope.whenTime+"\\n\\n"
					 +scope.infoLabelLocalized+"\\n"
					 +scope.voucherInfo+"\\n\\n";
		var details = scope.whatLabelLocalized+"\n"
			 +scope.voucherWhat+"\n\n"
			 +scope.whereLabelLocalized+"\n"
			 +where+"\n\n"
			 +scope.whenLabelLocalized+"\n"
			 +scope.whenDate+" "+scope.whenTime+"\n\n"
			 +scope.infoLabelLocalized+"\n"
			 +scope.voucherInfo+"\n\n";
		
		scope.icsEvent = {
	      filename: 'hm-event.ics',
	      url: window.location.href,
	      startTime: date,
	      endTime: endDate,
	      title: scope.voucherDescription,
	      description: details_ics,
	      address: where
	    };		
	
		scope.googleCalendarEventUrl = encodeURI("http://www.google.com/calendar/event?action=TEMPLATE" + 
				"&text=" + scope.voucherDescription + 
				"&details=" + details + 
				"&location=" + where +
				"&dates=" + date +"/" + endDate);
		
		scope.yahooCalendarEventUrl = encodeURI("http://calendar.yahoo.com/?v=60&view=m&type=20" + 
				"&title=" + scope.voucherDescription + 
				"&desc=" + details + 
				"&in_loc=" + where +
				"&st=" + date +
				"dur=" + scope.duration);
		
		scope.outlookCalendarEventUrl = encodeURI("https://bay01.calendar.live.com/calendar/calendar.aspx?rru=addevent" + 
				"&summary=" + scope.voucherDescription + 
				"&description=" + details +
				"&location=" + where +
				"&dtstart=" + date +
				"&dtend=" + endDate);
		
	};
	
	this.bookEvent = function(){
		
		var self = this;
		
		scope.isPointsRestoredAfterCancellation = false;	//if rebook just after cancellation
		scope.isPointsNoRestoredAfterCancellation = false;
		scope.spinnerOnBookingBugButton = true;
	
	
		hmClubNetService.addBooking(self.buildManageBookingPayload())
		.then(function(response){
		
			scope.spinnerOnBookingBugButton = false;
			hmCommon.closeModal('confirm-modal');
			
			var bookingResult = response.data;
			if(bookingResult && !bookingResult.error){
				
				scope.AmIBookedToThisOffer = true;
				scope.bookingId = bookingResult.bookingId;
				self.updateStatusAfterManageBooking();
				self.setRecapBookingInfo();
				self.setOfferMode("confirmation");
				self.scrollTop();
				
				
				if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE){
					self.loadBookingInfo(scope.bookingId, scope.venueCompanyId);
				}
			}else{
				if(bookingResult.error && bookingResult.error.reasonCode){
					console.log("ERROR: " + bookingResult.error.reasonCode);
					if("BOOKING_FAILURE"==bookingResult.error.reasonCode){
						hmCommon.openModal('error-modal-fully-booked');
					}else if("BOOKING_SYSTEM_UNAVAILABLE"==bookingResult.error.reasonCode){
						hmCommon.openModal('error-modal-connectivity-issue');
					//[PS] manage phone validation	
					}else if("BOOKING_VALIDATION_FAILURE"==bookingResult.error.reasonCode){
						scope.clubModel.personalDetails.invalidPhoneNumber = true;
						scope.isBookEventButtonEnabled = self.checkIfValidForBooking();
					}	
				}else{
					hmCommon.openModal('error-modal-connectivity-issue');
				}
			}
		},
		function(err){
			console.log(err);
			scope.spinnerOnBookingBugButton = false;
			hmCommon.closeModal('confirm-modal');			
			hmCommon.openModal('error-modal-connectivity-issue');
		});
	};
	
	
	this.setOfferMode = function(mode){
		
		//[PS] he we instances all the boolea that control the view on offer page. Note that we have isVenueEditable and isWhereEditable that are
		// the same semantic fields but in html page we have 2 different fields for event venues and where SA. so we need 2 different boolean.
		//Since we have only one label for those, we need also is StoresEditable that is the union of the 2s.
		if(mode=='booking'){
			scope.isValidationEnabled = true;
			scope.isOfferBookingMode = true;
	 		scope.isOfferConfirmationMode = scope.isOfferViewMode = scope.isOfferEditMode = false;	 
	 		scope.isVenuesEditable = scope.isMultipleVenueEvent;
	 		scope.isWhereEditable = true && (scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE );
	 		scope.isStoresEditable = (scope.isWhereEditable && scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE ) || 
	 									(scope.isVenuesEditable && scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_EVENT ) ;
	 		
	 		if((scope.clubModel.personalDetails.firstName+"").trim().length==0 && (scope.clubModel.personalDetails.lastName+"").trim().length==0){
	      		scope.clubModel.personalDetails.isInEditMode = true;
	      		if(scope.errorMobileNumberFormatText && scope.errorMobileNumberFormatText.length){
	    			scope.errorMobileNumberFormatText = scope.errorMobileNumberFormatText
	    			.replace("{0}", scope.clubModel.personalDetails.phoneNumberFormat);
	    		}
	    	}
	 		
	 		if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE){
	        	scope.isInterestedInEditable = true;
	        	scope.isWhenEditable = true;
	        }
	 		
	 		if(scope.offerType==scope.clubModel.OFFER_TYPE.SERVICE_PACKAGE){
	 			scope.showActiveMarker = false;
	 			scope.showServiceDuration = true;
	 			scope.showServiceValidUntil = false;
	 			if(scope.clubModel.currentlyActiveService.offerKey){
	 				scope.showVoucherAlreadyActiveBanner = true;
	 			}
	 		}
	 		
		}else if(mode=='view'){
			scope.isOfferViewMode = true;
	 		scope.isOfferBookingMode = scope.isOfferConfirmationMode = scope.isOfferEditMode = false;
	 		scope.clubModel.personalDetails.isInEditMode = false;
	 		scope.isInterestedInEditable = false;
	 		scope.isWhenEditable = false;
	 		scope.isVenuesEditable = false;
	 		scope.isWhereEditable = false;
	 		scope.isStoresEditable = false;
	 		if(scope.offerType==scope.clubModel.OFFER_TYPE.SERVICE_PACKAGE){
	 			scope.showActiveMarker = true;
	 			scope.showActivateButton = false;
	 			scope.showVoucherAlreadyActiveBanner = false;
	 			scope.serviceJustActivatedBanner=false;
	 			scope.showServiceDuration = false;
	 			scope.showServiceValidUntil = true;
	 		}
	 		
	 		
		}else if(mode=='confirmation'){
			scope.isOfferConfirmationMode = true;
			scope.isOfferBookingMode = scope.isOfferViewMode = scope.isOfferEditMode = false;
			scope.clubModel.personalDetails.isInEditMode = false;
			scope.isInterestedInEditable = false;
	 		scope.isWhenEditable = false;
	 		scope.isVenuesEditable = false;
	 		scope.isWhereEditable = false;
	 		scope.isStoresEditable = false;
	 		if(scope.offerType==scope.clubModel.OFFER_TYPE.SERVICE_PACKAGE){
	 			scope.showActiveMarker = true;
	 			scope.showActivateButton = false;
	 			scope.showVoucherAlreadyActiveBanner = false;
	 			scope.serviceJustActivatedBanner=true;
	 			scope.showServiceDuration = true;
	 			scope.showServiceValidUntil = false;
	 		}
			
		}else if(mode=='edit'){
			scope.isOfferEditMode = true;
			scope.isVenuesEditable = scope.multipleVenues;
			scope.isOfferBookingMode = scope.isOfferViewMode = scope.isOfferConfirmationMode = false;
			scope.isVenuesEditable = scope.isMultipleVenueEvent;
	 		scope.isWhereEditable = true && (scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE);
	 		scope.clubModel.personalDetails.isInEditMode = false;
			scope.isStoresEditable = (scope.isWhereEditable && scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE ) || 
				(scope.isVenuesEditable && scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_EVENT ) ;
			
			if((scope.clubModel.personalDetails.firstName+"").trim().length==0 || (scope.clubModel.personalDetails.lastName+"").trim().length==0){
	      		
	      		if(scope.errorMobileNumberFormatText && scope.errorMobileNumberFormatText.length){
	    			scope.errorMobileNumberFormatText = scope.errorMobileNumberFormatText
	    			.replace("{0}", scope.clubModel.personalDetails.phoneNumberFormat);
	    		}
	    	}
			
			if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE){
	        	scope.isInterestedInEditable = true;
	        	scope.isWhenEditable = true;
	        }
		}
		
		
	};
	
	this.scrollTop = function(){
		setTimeout(function(){
	    var top = 0;
	    $('html, body').animate({
	      scrollTop: top
	    }, 200);
	  }, 500);
	};
	
	//2016-03-22T13:12:01.333Z
	this.getGentleDate = function(d){
		if(d){
			return d.substring(0,10);
		}else{
			return '';
		}
	};
	
	this.getGentleTime = function(d){		
		if(d){
			return d.substring(11,16);
		}else{
			return '';
		}
	};
	
	this.getGentleTimeShift = function(d){		
		if(d){
			return d.substring(d.indexOf("+")+1);
		}else{
			return '';
		}
	};
	
	this.getGentleDateTime = function(d){		
			return this.getGentleDate(d);		
	};
	
	this.setRecapBookingInfo = function(){
		var phone = scope.clubModel.personalDetails.phonePrefix + " " + scope.clubModel.personalDetails.phoneNumber;
		
		if(scope.summaryCancellationTextInit && scope.summaryCancellationTextInit.length){
			scope.summaryCancellationText = scope.summaryCancellationTextInit
			.replace("{0}", "<strong>" + self.getGentleDateTime(scope.bookingLastCancellationDateTime)+" "+self.getGentleTime(scope.bookingLastCancellationDateTime) + "</strong>");
		}
		if(scope.summaryFriendsNameTextInit && scope.summaryFriendsNameTextInit.length){
			scope.summaryFriendsNameText = scope.summaryFriendsNameTextInit
			.replace("{0}", "<strong>" + scope.preferences.bringAFriendName + "</strong>");
		}
		if(scope.summarySmsNotificationTextInit && scope.summarySmsNotificationTextInit.length){
			scope.summarySmsNotificationText = scope.summarySmsNotificationTextInit
			.replace("{0}", "<strong>" + phone + "</strong>");
		}
		if(scope.summaryEmailNotificationTextInit && scope.summaryEmailNotificationTextInit.length){
			scope.summaryEmailNotificationText = scope.summaryEmailNotificationTextInit
			.replace("{0}", "<strong>" + scope.clubModel.personalDetails.email + "</strong>");
		}
		if(scope.summaryEmailSmsNotificationTextInit && scope.summaryEmailSmsNotificationTextInit.length){
			scope.summaryEmailSmsNotificationText = scope.summaryEmailSmsNotificationTextInit
			.replace("{0}", "<strong>" + phone + "</strong>")
			.replace("{1}", "<strong>" + scope.clubModel.personalDetails.email + "</strong>");
		}
		if(scope.comeMessageText && scope.comeMessageText.length){
			scope.comeMessageText = scope.comeMessageText
			.replace("{0}", scope.whenDate + " " + scope.whenTime);
		}
	};
	
	this.manageBBIsDown = function(arrayOfOffers){
		console.log("BB is DOWN!");
		scope.isBBDown = true;
    
		if(arrayOfOffers && arrayOfOffers.length){
			for(var i=0;i<arrayOfOffers.length;++i){
				var $item = arrayOfOffers[i],
					eventChainId = $item.data("club-event-chain-id"),
					serviceId = $item.data("club-service-id");
	  		if(eventChainId || serviceId){
	  			$item.addClass('disabled');
	  		}
			}								      	   
		};
		setTimeout(function(){
 			scope.$apply();
 		},0); 
	};
	
	this.refreshTimeSlotInfo = function(selectedTimeSlot, callback){
		
		var payload={ 
    		venueServiceId: scope.selectedStore.venueServiceId,
				venueCompanyId: scope.selectedStore.venueCompanyId,									
				date: scope.selectedDate.date
			},
			isTimeSlotValid = false;

		hmClubNetService.getServiceStoreTimeSlots(payload)
		.then(function(response){
			
			var timeSlots = response.data;
			if(timeSlots){
	 			for(var i=0;i<timeSlots.length;++i){
	 				if(timeSlots[i].timeSlot==selectedTimeSlot.timeSlot){	 						 					
	 					if(timeSlots[i].available){
	 						isTimeSlotValid = true;
	 					}
	 					break;
	 				}
	 			}	 				 			
		  }	 		
	 		callback(isTimeSlotValid);
		},
		function(err){
			self.manageBBIsDown();
			callback(false);
		});
	};
	
	this.setStoreAndTimeInfoInScope = function(){
		scope.selectedDateTimeText = $filter('date')(scope.selectedDate.dateObj, scope.dateFormatter) + " " + scope.selectedTimeSlot.time;
		scope.whenDate = scope.selectedDate.date;
		scope.whenTime = scope.selectedTimeSlot.time;
		scope.whereStoreTitle = scope.selectedStore.name;
		scope.whereStreet =  scope.selectedStore.address;
		scope.whereCity = scope.selectedStore.cityName;		
	};
	
	this.refreshVenueInfoFor = function(venueToBeRefreshed, callback){
		
		hmClubNetService.getEventDetails({ clubEventChainId: scope.clubEventChainId })
		.then(function(event){
			
	 		if(event && event.venues){
	 			
	 			scope.clubModel.loadedEvent = event;

	 			for(var i=0;i<event.venues.length;++i){
	 				if(event.venues[i].venueCompanyId==venueToBeRefreshed.venueCompanyId){	 						 					
	 					venueToBeRefreshed = event.venues[i];
	 					break;
	 				}
	 			}	 				 			
		  }	 		
	 		callback(venueToBeRefreshed);
		},
		function(err){
			self.manageBBIsDown();
			callback();
		});
	};
	
	this.setVenueInfoInScope = function(venue){
		scope.selectedVenueText = "";
		scope.whenDate = self.getGentleDate(venue.eventDateTime);
		scope.whenTime = self.getGentleTime(venue.eventDateTime);			
		scope.whereStoreTitle = venue.address.address1;
		scope.whereStreet =  venue.address.address2;
		scope.whereCity = venue.address.address3;
		scope.isEventFullyBooked = venue.fullyBooked;
		scope.isEventFewSeatsLeft = venue.fewSeatsAvailable && !venue.fullyBooked && !(venue.seatsLeft==1); //(!)		
		
		if(venue.seatsLeft==1 && scope.selectedVenue==null){	//after first load
			scope.isEventOnlyOneSeatLeft = true;
		}
		if(venue.seatsLeft<=1 && scope.selectedVenue!=null){
			scope.isEventOnlyOneSeatLeftAfterCheck = true;
		}else{
			scope.isEventOnlyOneSeatLeftAfterCheck = false;
		}
		scope.selectedVenue = venue;
		scope.venueCompanyId = venue.venueCompanyId;
	};
	
	this.setDateTimeVenueInfoInScope = function(venue){
		
		scope.whenDate = self.getGentleDate(venue.eventDateTime);
		scope.whenTime = self.getGentleTime(venue.eventDateTime);			
		
	};
	
	
	this.updateBooking = function(){
	
		var self = this;
		scope.spinnerOnSaveButton = true;
		
		hmClubNetService.updateBooking(self.buildManageBookingPayload(scope.bookingId))
		.then(function(response){
			
			scope.spinnerOnSaveButton = false;
			
			var bookingResult = response.data;
			if(bookingResult && !bookingResult.error){	
					
				scope.bookingId = bookingResult.bookingId;
				self.updateStatusAfterManageBooking();
				scope.isChangeSaved = true;	
				
				self.setOfferMode("view");
				self.scrollTop();
				
			}else{
				if(bookingResult.error && bookingResult.error.reasonCode){
					console.log("ERROR: " + bookingResult.error.reasonCode);
					if("BOOKING_FAILURE"==bookingResult.error.reasonCode){
						hmCommon.openModal('error-modal-fully-booked');
					}else if("BOOKING_SYSTEM_UNAVAILABLE"==bookingResult.error.reasonCode){
						hmCommon.openModal('error-modal-connectivity-issue');
					
					//[PS] manage phone validation	
					}else if("BOOKING_VALIDATION_FAILURE"==bookingResult.error.reasonCode){
						scope.clubModel.personalDetails.invalidPhoneNumber = true;
						 scope.isBookEventButtonEnabled = self.checkIfValidForBooking();
					}	
				}else{
					hmCommon.openModal('error-modal-connectivity-issue');
				}
			}
			
			setTimeout(function(){
	 			scope.$apply();
	 		},0);
		},
		function(err){
			console.log(err);
			scope.spinnerOnBookingBugButton = false;
			hmCommon.closeModal('confirm-modal');			
			hmCommon.openModal('error-modal-connectivity-issue');
		});
	};
	
	this.deleteBooking = function(){
		
		var self = this;
		scope.isFaultAfterCancellation = scope.isChangeSaved = false;
		
		hmClubNetService.deleteBooking({ bookingId: scope.bookingId, venueCompanyId: scope.venueCompanyId })
		.then(function(response){
			
			var bookingResult = response.data;
			if(bookingResult && bookingResult.error){
				scope.isFaultAfterCancellation = true;
			}else{
				scope.AmIBookedToThisOffer = false;
				
				//TODO: points restored or not should be server side computed and available in delete response
				var lastCancellationDate = new Date(scope.bookingLastCancellationDateTime);
				
				var lastRegistrationDate;
				if(scope.eventLastRegistrationDateTime){
					lastRegistrationDate = new  Date(scope.eventLastRegistrationDateTime);
				}
				else{
					lastRegistrationDate = moment(scope.lastRegistrationDateText).toDate();
				}
				
				var	now = new Date();
	
				if(lastCancellationDate>=now){
					scope.isPointsRestoredAfterCancellation = true;
				}else{
					scope.isPointsNoRestoredAfterCancellation = true;
				}		
				
	
				if(lastRegistrationDate>=now){
					self.setOfferMode("booking");
					var _bringAFriendVisible = scope.preferences.bringAFriendVisible
					scope.preferences = {
		  				smsAlertRequired: false,
		  				mailAlertRequired: false,
		  				bringAFriendChecked: false,
  						bringAFriendName: "",
  						bringAFriendVisible: _bringAFriendVisible
		  			};
					
					//[PS] we suppose that if i cancel an event that is fully booked, event now has 1 few seats left.
					if(scope.isEventFullyBooked){
						scope.isEventFullyBooked = false;
						scope.isEventFewSeatsLeft = false;
						scope.isEventOnlyOneSeatLeft= true;
						scope.isRedeemButtonHidden = false;
						scope.preferences = {
								bringAFriendVisible: false
						}
						
					}
					
					self.cleanSelectedStore();
					
					scope.isBookEventButtonEnabled = self.checkIfValidForBooking();	 
					
				}else{
					self.setOfferMode("view");
				}
				self.scrollTop();
				
				setTimeout(function(){
		 			scope.$apply();
		 		},0);
			}
		},
		function(err){						
			scope.isFaultAfterCancellation = true;
		});

	};
	
	this.cleanSelectedStore = function(){
		var self = this;
		scope.selectedCityText = scope.selectedCityTextInitial;
		scope.selectedStore = null;
		
		for(var i=0; i<scope.clubModel.storesInCity.length;++i){
			var store = scope.clubModel.storesInCity[i];
			
				store.selected=false;
				break;
			

		}
		self.cleanSelectedDateTime();
		scope.whenButton.attr('disabled','');
	};
	
	this.cleanSelectedDateTime = function(){
		scope.selectedDateTimeText = scope.selectedDateTimeTextInitial;
 		
	};
		
}]);	//module end

/**
 * Networking Mocks
 * 
 */
angular.module('hmApp').factory('hmClubNetServiceMock', ['$rootScope', '$http', function($rootScope, $http){

		var uris = (function(){

		var origin = window.location.origin;
	  if (!origin) {
	    origin = window.location.protocol + "//" + window.location.hostname + (window.location.port ? ':' + window.location.port: '');
	  }
	  origin = origin.replace('http:','https:');

	  var locale,
	  	countryCode,
	    regex = new RegExp(/[a-z]*_[A-Z]*/i),
	    matches = window.location.pathname.match(regex);
	  if(matches && matches.length) {
	    locale = matches[0];
	  }
	  var locSplit = locale.split("_");
	  locale = locSplit[0] + "_" + locSplit[1].toUpperCase();
	  countryCode = locSplit[1].toUpperCase();  	  
	  var base = origin + '/' + locale + '/loyalty/';

	  $rootScope.locale = locale;
	  $rootScope.countryCode = countryCode;

	  var uris = {};
//	  uris.storesInfo = '/rest/storelocator/stores/1.0/locale/'+locale+'/country/'+countryCode+'/';
	  uris.storesInfo = 'http://dev-m2.hm.com/rest/storelocator/stores/1.0/locale/pl_PL/country/PL/';
	  return uris;     
	})();
	
	return {
