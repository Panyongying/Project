
/**
 * Common utilities, meant to be reusable across multiple controllers
 * 
 */
angular.module('hmApp').service('hmCommon', ['$rootScope', function($rootScope){

  this.USER_COOKIE = 'userCookie';
  this.hmApi = hm;
  this.useMockData = false;	//pier debug  
  
	console.logp = function(str){	  
	  console.log("%c [club]: " + str, "color:white; background:blue; margin:2px;");
	};
	
	this.openModal = function(modalId){
		(function(mm){
			var modal = mm.getModal(modalId);
			if(modal){
				mm.pushModal(modal);
			}else{
				mm.loadModalWithElement(modalId, $('#'+modalId));
			}
		})(this.hmApi.modalManager);		
	}
	
	this.closeModal = function(modalId){
		(function(mm){
			mm.popModal(mm.getModal(modalId));
		})(this.hmApi.modalManager);		
	}
}]);


/**
 * Cookies manager, meant to be reusable across multiple controllers
 * 
 */
angular.module('hmApp').service('hmCookies', function(){

  this.getCookieValue = function(cookieName){
  	var cookieAll = document.cookie.split(';'),
      cookieValue;     
    if(cookieAll && cookieAll.length){
    	for(var i=0;i<cookieAll.length;++i){
    		var cookiePair = cookieAll[i].replace(/^\s+|\s+$/g, '').split('=');	//IE8 does not either supports trim()
    		if(cookiePair[0]==cookieName){          
          cookieValue = decodeURIComponent(cookiePair[1]);
          break;
        } 
    	}    
    }    
    return cookieValue;
  };

  this.getCookieObject = function(cookieName){
  	var cookieValue = this.getCookieValue(cookieName),
  		cookieObject = null;
  	if(cookieValue){
  		try{
				cookieObject = JSON.parse(eval(cookieValue));
  		}
  		catch(err){
  			cookieObject = JSON.parse(cookieValue);
  		}
  	}
  	return cookieObject
  };
});


/**
 * Way to use Js calendar CLNDR as it is defined in aplication.js,
 * hm.calendar is Global object defined in application.js with hm.Calendar constructor
 * it should be instantiated before Angular stuff loads (and usually it is)
 * 
 */
angular.module('hmApp').service('hmBookingCalendar', ['$rootScope', 'hmCommon', 'hmClubNetService','hmClubNetServiceMock', function($rootScope, hmCommon, hmClubNetService, hmClubNetServiceMock){

	this.scope={};
	this.eventArray = [];
	
	if(hmCommon.useMockData){
 		hmClubNetService = hmClubNetServiceMock;
 	}
	
  this.refreshCalendar = function(){
   	var self = this;

   	//hm is globally defined in application.js
  	hm.Calendar = function() {
  		var publ = this;
	    var priv = {
	        elementSelectors: {
	            clndrElement: '#calendar',
	            avlTimesElement: '#available-times .selectable-items'
	        },
	        selectedClass : 'selected',
	        clndr: this.element
	    };        
	
	    priv.init = function(){
	        priv.calendarEvents();
	    }; 
	
	    priv.calendarEvents = function(e){	        
	        priv.renderCalendar(self.eventArray);
	    }
	
	    priv.renderCalendar = function(events) {
	    		
        if ($(priv.elementSelectors.clndrElement).length) {
      		var cal = hm.calendar.renderCalendar(events);
      		
          cal.options.clickEvents.click = function (target) {
          	
          	var clndr = this,
          		payload={ 
	          		venueServiceId: self.scope.selectedStore.venueServiceId,
								venueCompanyId: self.scope.selectedStore.venueCompanyId,									
								date: target.events[0].date,
								dateObj: target.date.toDate()	
							};

						hmClubNetService.getServiceStoreTimeSlots(payload)
						.then(function(response){
							
							var timeSlots = response.data;
							if(timeSlots){
								target.events = [];
								for(var i=0;i<timeSlots.length;++i){
									var t = timeSlots[i].timeSlot.split(':'),
										item = { date: payload.date, 
															id: i, 
															time: t[0]+':'+t[1],
															timeSlot: timeSlots[i].timeSlot,
															duration: timeSlots[i].duration
														};
									if(timeSlots[i].available){
										item.disable = "";
										item.error = "";
									}else{
										item.disabled="disabled";
										item.error=self.scope.fullyBookedDateMessage;
									}								
									target.events.push(item);
								}
								self.scope.selectedDate = { date: payload.date,
															timeSlots: target.events,
															dateObj: payload.dateObj};
								setTimeout(function(){
									self.scope.$apply();
								},0);
								
								if(target.events.length) {
		              $('.day.event').removeClass(priv.selectedClass);		              
		             	$(target.element).addClass(priv.selectedClass);
		            }
		          }
						},
						function(err){
							self.scope.isLosDown = true;
							setTimeout(function(){
								self.scope.$apply();
							},0);
							
						});
          }        
        }
	    }
	    publ.downloadCalendar = function(event, e) {
	        var 
	            icsData,
	            blob,
	            filename = event.filename,
	            ics  = [ 
	              'BEGIN:VCALENDAR',
	              'VERSION:2.0',
	              'BEGIN:VEVENT',
	              'UID:' + event.id,
	              'URL:' + event.url,
	              'DTSTART:' + (event.startTime || ''),
	              'DTEND:' + (event.endTime || ''),
	              'SUMMARY:' + (event.title || ''),
	              'X-ALT-DESC;FMTTYPE=text/html:' + (event.description || ''),
	              'LOCATION:' + (event.address || ''),
	              'END:VEVENT',
	              'END:VCALENDAR'].join('\n');

	        if('msSaveBlob' in window.navigator) {
	            blob = new Blob([ics]);
	            navigator.msSaveBlob(blob, filename);
	            e.preventDefault();

	        } else {
	            icsData = 'data:text/calendar;charset=utf8,' + encodeURI(ics);
	            $(e.currentTarget).attr({
	                "href": icsData,
	                "target": "_blank",
	                "download": filename
	            });
	        }
	    };
	    priv.init();
		};
		hm.calendar = new hm.Calendar();
  };

}]);



/**
 * Networking
 * 
 * here are defined all the rest api call that supports club actions
 * all the calls are sent through adobe to Hybris
 * which in turn calls LOS or BB systems
 */
angular.module('hmApp')
	.config(function ($httpProvider) {	  
  	$httpProvider.defaults.withCredentials = true; 
  })
  .factory('hmClubNetService', ['$rootScope', '$http', function($rootScope, $http){

	var uris = (function(){
	  
		var origin = window.location.origin;		
	  if (!origin) {
	    origin = window.location.protocol + "//" + window.location.hostname + (window.location.port ? ':' + window.location.port: '');
	  }
	  origin = origin.replace('http:','https:');

	  var locale,
	  	localeUC,
	  	countryCode,
	    regex = new RegExp(/[a-z]*_[A-Z]*/i),
	    matches = window.location.pathname.match(regex);
	  if(matches && matches.length) {
	    locale = matches[0];
	  }
	  //[PS] added to manage master mobile whose locale is only en
	  else{
		  locale = "en_us";
	  }
	  var locSplit = locale.split("_");
	  locale = locSplit[0] + "_" + locSplit[1];	  
	  localeUC = locSplit[0] + "_" + locSplit[1].toUpperCase();
	  countryCode = locSplit[1].toUpperCase();
	  
	  var language = locSplit[0].toLowerCase(),
	  languageUC = language.toUpperCase();
	  var base = origin + '/' + locale + '/loyalty/';

	  $rootScope.urlBase = base; 
	  $rootScope.locale = locale;
	  $rootScope.language = language;
	  $rootScope.countryCode = countryCode;
	  

	  var uris = {};
	  uris.loyaltyMemberServiceUri = base + 'getMember';   	     
	  uris.loyaltyOffersServiceUri = base + 'getOffersProposition';
	  uris.loyaltyRedeemOffersServiceUri = base + 'redeemOffer';
	  uris.storesInfo = '/rest/storelocator/stores/1.0/locale/' + localeUC + '/country/' + countryCode + '/';
	  
	  uris.eventsServiceUri = base + 'booking/events';
	  uris.eventDetailsServiceUri = base + 'booking/eventDetails';
	  uris.bookingsServiceUri = base + 'booking/bookings';
	  uris.bookingDetailsServiceUri = base + 'booking/bookingDetails';
	  uris.manageBookingServiceUri = base + 'booking/manageBooking';
	  uris.deleteBookingServiceUri = base + 'booking/cancelBooking';
	  	  
	  uris.bookingServicesUri = base + 'booking/services';
	  uris.offerStoreDatesServiceUri =  base + 'booking/serviceDates';
	  uris.offerStoreTimeSlotsServiceUri = base + 'booking/serviceTimeslots';	  
	  return uris;     
	})();

	return {
		
		//LOS
    getMember: function(payload){    	
    	return $http.get(uris.loyaltyMemberServiceUri, { params: payload });
    },
    getOffersProposition: function(payload){	
      return $http.get(uris.loyaltyOffersServiceUri, { params: payload });		    
		},
    redeemOffer: function(payload){	    	
      return $http({
				method: 'POST',
				url: uris.loyaltyRedeemOffersServiceUri,
				headers: { 
					'Content-Type': 'application/x-www-form-urlencoded'
				},	
				transformRequest: function(payload) {
	        var str = [];
	        for(var p in payload){
		        str.push(encodeURIComponent(p) + "=" + encodeURIComponent(payload[p]));
		      }
	        return str.join("&");
		    },
				data: payload,
			});      		    
		},
		
		//Store Locator
		getStoresInfo: function(){			
    	return $http({
				method: 'GET',
				url: uris.storesInfo,
				headers: { 
					'Content-Type': 'application/xml',
					'Accept': 'application/xml'
				},
				withCredentials: false,
				dataType:'xml'
			});
    },
    
    //Booking Bug
		getEvents: function(payload){				
      return $http.get(uris.eventsServiceUri, { params: payload });	    
		},
		getEventDetails: function(payload){				
      return $http.get(uris.eventDetailsServiceUri, { params: payload });	    
		},
		getBookings: function(){						
      return $http.get(uris.bookingsServiceUri);	    
		},
		getBookingDetails: function(payload){				
      return $http.get(uris.bookingDetailsServiceUri, { params: payload });	    
		},
    getServices: function(payload){	
      return $http.get(uris.bookingServicesUri, { params: payload });	    
		},
		getServiceStoreDates: function(payload){	
      return $http.get(uris.offerStoreDatesServiceUri, { params: payload });	    
		},
		getServiceStoreTimeSlots: function(payload){				
      return $http.get(uris.offerStoreTimeSlotsServiceUri, { params: payload });	    
		},
		addBooking: function(payload){
			return $http({
				method: 'POST',
				url: uris.manageBookingServiceUri,
				headers: { 
					'Content-Type': 'application/x-www-form-urlencoded'
				},	
				transformRequest: function(payload) {
	        var str = [];
	        for(var p in payload){
		        str.push(encodeURIComponent(p) + "=" + encodeURIComponent(payload[p]));
		      }
	        return str.join("&");
		    },
				data: payload
			});
		},
		updateBooking: function(payload){
			return $http({
				method: 'POST',
				url: uris.manageBookingServiceUri,
				headers: { 
					'Content-Type': 'application/x-www-form-urlencoded'
				},	
				transformRequest: function(payload) {
	        var str = [];
	        for(var p in payload){
		        str.push(encodeURIComponent(p) + "=" + encodeURIComponent(payload[p]));
		      }
	        return str.join("&");
		    },
				data: payload
			});
		},
		// Pier says:
		// do not use 'return $http.delete(uris.deleteBookingServiceUri, { params: payload });'
		// if compressed with YUI, delete conflicts with native delete keyword in minification!
		deleteBooking: function(payload){
			//return $http.delete(uris.deleteBookingServiceUri, { params: payload });
			return $http({
				method: 'DELETE',
				url: uris.deleteBookingServiceUri,
				params: {
					bookingId: payload.bookingId,
					venueCompanyId: payload.venueCompanyId
				}
			});
		}
	};
}]);





/**
 * Club service - (ex R5 Loyalty)
 * 
 * This i s the core Club moduel which is an Angular Service Module 
 * (service is a module term in Angular framework, has nothing directly 
 * related to the term "service" in BB which is a kind of offer (ZR04).
 * 
 * The module has dependency on other modules and occasionally relies on JQuery DOM manipulation 
 * because of:
 * 1) This is a porting of the Loyalty() JQuery implementation
 * 2) There are dependency on code in application.js provided from HTML team,
 * 		i.e. all the Modals are managed within application.js anc can't be moved 
 * 		to pure Angular implementation.
 * 
 * This module is also loaded in the Hybris part of the project, i.e. cart, checkout
 * because it manages club url rewrite for the whole site pages 
 * 
 * the model (M in MVC) defined in this module is core club, the module also modify part of the model defined in controller
 * the initClub is the entry point triggered by the watcher in controller, that checks for hmClubEnabled to be true
 * otherwise nothins is instantiated
 * 
 * 
 */
angular.module('hmApp').service('hmClubService', ['$rootScope', '$timeout', 'hmCommon', 'hmCookies', 'hmClubNetService', 'hmBookingCalendar','$filter', 'hmClubNetServiceMock',
																					function($rootScope, $timeout, hmCommon, hmCookies, hmClubNetService, hmBookingCalendar, $filter, hmClubNetServiceMock){

	var scope = {},
		self = this;
	
	this.initClub = function($scope){

		if(hmCommon.useMockData){
				hmClubNetService = hmClubNetServiceMock;
			}
		
		scope = $scope;	
		scope.isClubFullMemberPage = false;
		scope.storageName = "localStorageMemberInfo";
		scope.offerPropositionIdsName = "hmClubOfferPropositionIds";
		scope.$vouchers = $('.voucherslisting').first();
		scope.$vouchersnooffers = $('.vouchersnooffers');		
		scope.$vouchersservicedown = $('.vouchersservicedown');
		scope.selectedVenueTextInitial = scope.selectedVenueText;
		scope.offerPropositionIds = null;
	  
		scope.userStatus = "NON_LOGGED";

		scope.clubModel = {
	    urlMapping: { 
	      INITIAL: "initialmember",
	      NON_MEMBER: "customer",
	      SUBSCRIBER: "customer",
	      FULL_MEMBER: "fullmember",
	      PENDING: "pendingmember",
	      PENDING_ABORTED: "pendingmember",
	      NON_LOGGED: "guest"
	    },
	    rewardsUrlMapping: { 
		      FULL_MEMBER: "fullmember",
		      PENDING: "pendingmember",
		      PENDING_ABORTED: "pendingmember"
		    },
	    linkIds: ['hmClubAccountSignedInDynamicLink', 
	              'hmClubAccountNotSignedInDynamicLink', 
	              'hmClubDynamicLink',
	              'hmClubRewardsViewMoreLink'],
	    linkRewardsId: "hmClubRewardsDynamicLink",
	    mobileLinkRewardsId:'hmClubRewardsFromPointsCmp',
	    pagesMapping: { 
	      INITIAL: "clubpageinitialmember",
	      NON_MEMBER: "clubpagecustomer",
	      SUBSCRIBER: "clubpagecustomer",
	      FULL_MEMBER: "clubpagefullmember",
	      PENDING: "clubpagependingmember",
	      PENDING_ABORTED: "clubpagependingmember",
	      NON_LOGGED: "clubpageguest"
	    },
	    REDEEM_MODE: {
	      TOUCH: 2,
	      ACTUAL_REDEEM: 3
	    },
	    OFFER_TYPE: {
	    	REDEEM_INSTORE: {
	    		ZR01: true,
	    		ZR02: true,
	    		ZR07: true,
	    		ZR10: true,
	    		ZR11: true
	    	},
	    	CLUB_EVENT: "ZR03",
	    	CLUB_SERVICE: "ZR04",
	    	SERVICE_PACKAGE: "ZR13"
	    },
	    personalDetails: {
	    		name: "",
	  	 		email: "",
	  	 		phonePrefix: "",
	  	 		phoneNumber: "",
	  	 		phoneNumberFormat: "55555",	//need to be specified in some way server side, maybe in adobe with an ng-init
	  	 		fullPhoneNumber: "",
	  	 		isInEditMode: false,
	  	 		invalidPhoneNumber:false
	    },	 		
	    cities: [],
	    storesByCity: {},
		  stores: {},
		  departments: {},
			offerDepartments: {},
		  offerCities: [],
		  offerStores: [],
		  filteredOfferStores: [],
		  storesInCity: [],	//service
		  sceventVenues: [],		//event
		  loadedEvent: null,
		  loadedBooking: null,
		  currentlyActiveService: {}
	  };


	  if(scope.hmClubServiceCacheDuration===undefined){
	    scope.hmClubServiceCacheDuration = 0; //10*60*1E3;
	  } else{
	    scope.hmClubServiceCacheDuration = scope.hmClubServiceCacheDuration * 6E4;
	  }

		switch(scope.pageType){
		  case "clubpagefullmember":
		    scope.isClubFullMemberPage = true;
		    break;
		  case "clubpagecustomer":
		    scope.isHmcPointsAvailableVisible = true;
		    break;
		  case "clubpageinitialmember":
		    scope.isHmcPointsAvailableVisible = true;
		    break;
		  case "clubpagependingmember":
		    scope.isHmcPointsAvailableVisible = true;
		    break;
		  case "clubpageguest":
		    scope.isClubGuest = true;
		    scope.isHmcPointsAvailableVisible = true;
		    break;
		  case "offerpage":
		    scope.isClubOfferPage = true;
		    break;  
		  default:
		    scope.isGenericPage = true;
		    scope.isHmcPointsAvailableVisible = true;
		};		

		this.initModalAccordions();
		this.retrieveUserInfo();
	};

	this.retrieveUserInfo = function(){

		var userCookieObj = hmCookies.getCookieObject(hmCommon.USER_COOKIE);

	  if(userCookieObj && userCookieObj.hybris_uuid){
				
			scope.hybris_uuid = userCookieObj.hybris_uuid;
	  	console.logp("isLoggedIn: " + scope.hybris_uuid);  

			var needToRefreshUserInfo = true;

	    if(scope.isGenericPage){
	      var memberInfo = JSON.parse(localStorage.getItem(scope.storageName));        
	      if(!!memberInfo 
	        && memberInfo.emailAddress == userCookieObj.hybris_uuid 
	        && (Date.now() - memberInfo.clientTime) < scope.hmClubServiceCacheDuration)
	      {          
	        needToRefreshUserInfo = false;
	      }
	    }

	    if(needToRefreshUserInfo)
	    { 
	      this.doGetMemberCall();      
	    }else{               
	      this.getMemberInfoFromWebStorage(memberInfo);      
	    }      
	  }else{
	    this.notLoggedIn();
	    console.log("User is NOT LoggedIn");
	  }
	};

	this.getMemberInfoFromWebStorage = function(memberInfo) {
	  this.applyMemberData(memberInfo);
	};

	this.notLoggedIn = function(){
	  this.checkIfRedirectionIsNeeded(scope.userStatus = "NON_LOGGED");    
	  this.setHmClubLink();
	};
	
	this.setHmClubLink = function() {  

		var self = this;
		
		for(var i=0;i<scope.clubModel.linkIds.length;++i){
			var id = scope.clubModel.linkIds[i],
				$hmClubDynamicLinkAnchor = $('#' + id);
			
		    if($hmClubDynamicLinkAnchor.length){
		      var anchor = $hmClubDynamicLinkAnchor[0],
		      $anchor = $(anchor),
		      href = $anchor.attr("href"),
		      replacedHref = href.replace('selector-placeholder', (function(v){           
		        return (v[scope.userStatus]) ? v[scope.userStatus] : v['NON_LOGGED'];
		      })(scope.clubModel.urlMapping));      
		      $anchor.attr("href", replacedHref);
		    }
		}
			
		var $hmClubRewardsDynamicLinkAnchor = $('#' + scope.clubModel.linkRewardsId);
		var $hmClubMobileRewardsDynamicLinkAnchor = $('#' + scope.clubModel.mobileLinkRewardsId);
		if($hmClubRewardsDynamicLinkAnchor.length){
			self.manageRewardsUrl($hmClubRewardsDynamicLinkAnchor);
		}
		if($hmClubMobileRewardsDynamicLinkAnchor.length){
			self.manageRewardsUrl($hmClubMobileRewardsDynamicLinkAnchor);
		}
		
	};
	
	this.manageRewardsUrl = function($anchor){
		var rewardsAlternativeUrl = $anchor.data("alternative-href"),
		rewardsHref = $anchor.attr("href"),
		rewardsReplacedHref = scope.clubModel.rewardsUrlMapping[scope.userStatus]?rewardsHref.replace('selector-placeholder', (function(v){           
        return (v[scope.userStatus]) ? v[scope.userStatus] : v['NON_LOGGED'];
      })(scope.clubModel.rewardsUrlMapping)):rewardsAlternativeUrl;      
		$anchor.attr("href", rewardsReplacedHref);
	}

	this.doGetMemberCall = function() {

		var self = this;
	  localStorage.removeItem(scope.storageName);

	  var payload = {        
	    emailAddress : scope.hybris_uuid,
	    corporateBrandId : 0,
	    countryCode : $rootScope.countryCode
	  };

	  scope.spinnerOnPersonalDetails = true;
	  hmClubNetService.getMember(payload).then(function(response){
	  	var data = response.data;
	  	scope.spinnerOnPersonalDetails = false;
	 		if(data){
	 			data.clientTime = Date.now();
				try{		      
		      localStorage.setItem(scope.storageName, JSON.stringify(data));    
		    }catch(err){}    
		    self.applyMemberData(data);
		  }else{
		  	self.setHmClubLink();
	      if(scope.isClubOfferPage){
	        self.getMemberInfoFromWebStorage(JSON.parse(localStorage.getItem(scope.storageName)));
	      }
		  }	 		
		},
		function(err){
			scope.spinnerOnPersonalDetails = false;
			scope.isLosDown = true;
			setTimeout(function(){
				scope.$apply();
				self.scrollTop();
			},0);	
		});
	};
	
	this.applyMemberData = function(data) {
	  
		var self = this;		
		self.checkIfRedirectionIsNeeded(data.status);
	  scope.userStatus = data.status;
	  scope.isUpToDate = data.upToDate;
	  scope.businessPartnerId = data.businessPartnerId;	  
	  scope.customerLoyaltyId = data.customerLoyaltyId;
	  scope.alreadyActiveOSPKey = data.customerLoyaltyId+"_currently_active_service_package";
	  scope.clubModel.personalDetails.firstName = (data.firstName) ? data.firstName : "";
	  scope.clubModel.personalDetails.lastName = (data.lastName) ? data.lastName : "";
	  scope.clubModel.personalDetails.name = scope.clubModel.personalDetails.firstName + " " + scope.clubModel.personalDetails.lastName;
	  scope.clubModel.personalDetails.email = data.emailAddress;
	  scope.clubModel.personalDetails.phonePrefix = (data.prefixPhoneNumber) ? data.prefixPhoneNumber.replace(/\s/g,'') : "";
 		scope.clubModel.personalDetails.phoneNumber = (data.phoneNumber) ? data.phoneNumber.replace(/\s/g,'') : "";
 		scope.clubModel.personalDetails.fullPhoneNumber = scope.clubModel.personalDetails.phonePrefix + " " + scope.clubModel.personalDetails.phoneNumber;
	  
 		scope.offerKeyOnRedemptionNow = JSON.parse(localStorage.getItem('hmc_stored_currently_redeem_offer_id'));
 		
 		if(localStorage.getItem(scope.alreadyActiveOSPKey)){
			scope.clubModel.currentlyActiveService = JSON.parse(localStorage.getItem(scope.alreadyActiveOSPKey));
		}

 		self.setHmClubLink();
 		
// 		console.log("CANCELLAMI: scope.isUpToDate = true;");
// 		scope.isUpToDate = true;
	  
	  if(!scope.isUpToDate){    
	    $('#hmc_points_unavailabe_message').show();
	    $('.eancodecard section h2.sub-heading').hide();
	    $('.eancodecard section strong.points').hide();
	    $('.eancodecard section div.ean-code-wrapper').css('margin-top','30px');    
	  }
	  else if(scope.isClubOfferPage){
	  	
		  (function(resolve) {
		  	
		  	//scope.offerKey is set in markUp with ng-init		  	
		  	scope.offerPropositionIds = JSON.parse(localStorage.getItem(scope.offerPropositionIdsName));	    	
	  		
		  	//if offerPropositionIds are not in local storage we are in an OfferPage direct access (without accessing the club page in advance)
		  	//we need to retrieve the offerPropositionIds before going any further
		    if(localStorage.getItem(scope.offerPropositionIdsName)){		    	
		      resolve();
		    }else{
		      self.fetchAvailableOffers(resolve);
		    }
		  })(function(){
	    	
			
		  	if(scope.offerPropositionIds && scope.offerPropositionIds[scope.offerKey]){
			  	scope.offerPropositionId = scope.offerPropositionIds[scope.offerKey].offerPropositionId;
		    	scope.offerDiscountCodeStore = scope.offerPropositionIds[scope.offerKey].offerDiscountCodeStore;
		    	if(scope.offerDiscountCodeStore){
		    		scope.offerDiscountCodeStoreEanUrl = $rootScope.urlBase + "barcode/" + scope.offerDiscountCodeStore;
		    	}
		  	}
		  	
		  	//[PS] i have to check if i have offer in redeem now and put info in scope to show ean
		  	if(localStorage.getItem("hmc_stored_currently_redeem_offer_proposition_id")){
		  		var myOfferInRedeem = JSON.parse(localStorage.getItem("hmc_stored_currently_redeem_offer_proposition_id"));
		    	scope.offerDiscountCodeStore = myOfferInRedeem.offerDiscountCodeStore;
		    	if(scope.offerDiscountCodeStore){
		    		scope.offerDiscountCodeStoreEanUrl = $rootScope.urlBase + "barcode/" + scope.offerDiscountCodeStore;
		    	}
		  	}
		  	
	    	//we get params from query string the old way, $location.search() is unreliable when not using routing    	
	    	var params = (function(){
				    var vars = [], hash;
				    var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
				    for(var i = 0; i < hashes.length; i++)
				    {
				        hash = hashes[i].split('=');
				        vars.push(hash[0]);
				        vars[hash[0]] = hash[1];
				    }
				    return vars;
				})();
	    	if(params.bid && params.vcid){
	    		scope.clubModel.vcid=params.vcid;
	    		scope.clubModel.bid=params.bid;
	    		self.loadBookingInfo(params.bid, params.vcid);
	    		scope.offerMode= "view";
	    	}else if(scope.offerType==scope.clubModel.OFFER_TYPE.SERVICE_PACKAGE){
	    		scope.offerMode = "booking";
	    		if (scope.clubModel.currentlyActiveService){
	    			var currentServiceKey = scope.clubModel.currentlyActiveService.offerKey;
	    			if(scope.offerKey == currentServiceKey){
	    				scope.currentServiceValidUntil = scope.clubModel.currentlyActiveService.endDate;
	    				scope.offerMode = "view";
	    			}
	    		}
	    	}else{
	    		scope.offerMode = "booking";
	    		
	    	}
	    	
	    	
	    	if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_EVENT){					
	    		
	    		//CLUB EVENT
	    		console.log("I'm in the: " + scope.clubModel.OFFER_TYPE.CLUB_EVENT);
	    		
	    		if(scope.clubEventChainId){
		    		scope.spinnerOnBookingDetails = true;
		    		
		    		hmClubNetService.getEventDetails({ clubEventChainId: scope.clubEventChainId })
		    		.then(function(response){	    			
		    			var event = response.data;
		    			
		    			scope.spinnerOnBookingDetails = false;
		    			
		    			scope.preferences.bringAFriendVisible = event.bringFriends==1 ? true : false;
		    			
		    	 		if(event && event.venues){
		    	 			
		    	 			scope.clubModel.loadedEvent = event;
		    	 			scope.bookingLastCancellationDateTime = event.lastCancellationDateTime;
		    	 			scope.eventLastRegistrationDateTime = event.lastRegistrationDateTime;
		    	 			if(scope.lastRegistrationDateText && scope.lastRegistrationDateText.length){
		    	 				scope.lastRegistrationDateText = scope.lastRegistrationDateText.replace('{0}', self.getGentleDateTime(event.lastRegistrationDateTime));
		    	 			}
		    	 				    	 
		    	 			scope.isOfferAfterRegistrationDate = (function(lrdt){
		    	 				var lastRegistrationDate = new  Date(lrdt),
		    	 				now = new Date();
		    	 				if(lastRegistrationDate>now){
		    	 					return false;
		    	 				}
		    	 				return true;
		    	 			})(scope.eventLastRegistrationDateTime);
		    	 			
		    	 			scope.isRegistrationClosed = !event.registrable;
		    	 			
		    	 			if(event.venues.length==1)
		    	 			{    	 				    	 				
		    	 				self.setVenueInfoInScope(event.venues[0]);    	 				
		    	 				scope.isBookEventButtonEnabled = self.checkIfValidForBooking();	 
		    	 				if(scope.isEventFullyBooked){
		    	 					scope.isRedeemButtonHidden=true;
		    	 				}
		    	 			}
		    	 			else if(event.venues.length>1)	//Multiple location event
		    	 			{
		    	 				scope.isMultipleVenueEvent = true;
		    	 				scope.clubModel.eventVenues = [];
		    	 				for(var i=0;i<event.venues.length;++i){
		    	 					var venue = event.venues[i];
		    	 					venue.title=venue.address.address1;
		    	 					venue.street=venue.address.address2;
		    	 					venue.city=venue.address.address3;
		    	 					venue.dateOfEvent=self.getGentleDateTime(venue.eventDateTime);
		    	 					scope.clubModel.eventVenues.push(venue);
		    	 				}  	
		    	 				self.setDateTimeVenueInfoInScope(event.venues[0]);   
		    	 				scope.isBookEventButtonEnabled = self.checkIfValidForBooking();
		    	 			}    	 			
		    		  } 
		    	 		self.setRecapBookingInfo();

                        scope.isBookEventButtonEnabled = self.checkIfValidForBooking();	
                        
                        self.setOfferMode(scope.offerMode);
		    	 		
		    	 		setTimeout(function(){
		    	 			scope.$apply();
		    	 		},0);    	 		
		    		},
		    		function(err){
		    			scope.spinnerOnBookingDetails = false;
		    			self.manageBBIsDown();
		    		});
		    	}
	    	}    	    	   
		    else if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE){							
		    	
		    	//CLUB SERVICE
		  		console.log("I'm in the: " + scope.clubModel.OFFER_TYPE.CLUB_SERVICE);	
		  		self.initStoresInfo();
		  		
		  		self.setOfferMode(scope.offerMode);
		  		
		  		setTimeout(function(){
	  	 			scope.$apply();
	  	 		},0); 
					
		  	}else if(scope.clubModel.OFFER_TYPE.REDEEM_INSTORE[scope.offerType]){							
		  		
		  		//IN STORE OFFER
		  		//console.log("I'm in the: " + scope.offerType);
		  		self.setOfferMode(scope.offerMode); 		
		  	
		  	}else if(scope.offerType==scope.clubModel.OFFER_TYPE.SERVICE_PACKAGE){							
		  		
		  		//SERVICE PACKAGE
		  	   //console.log("I'm in the: " + scope.offerType);
		  	  //if user is viewing his currently active offer currentServiceValidUntil is populated
			  if(scope.currentServiceValidUntil){
				  if(scope.servicePackageValidUntilText && scope.servicePackageValidUntilText.length){
			        	scope.servicePackageValidUntilText = scope.servicePackageValidUntilText.replace("{0}", self.getGentleDate(scope.currentServiceValidUntil));
			        } 
					if(scope.lastRegistrationDateText && scope.lastRegistrationDateText.length){
	  	 				scope.lastRegistrationDateText = scope.lastRegistrationDateText.replace('{0}', self.getGentleDateTime(scope.currentServiceValidUntil));
	  	 			}
			  }
			  else{
				  $(".valid").hide();
			  }
			  
			  scope.OSPPopupId="#redeem_osp_confirm-modal";
			  if(scope.isLosDown || !scope.isUpToDate){
				  scope.OSPPopupId="#los_is_down-modal";
			  }
			  
			  scope.redeemPoints = parseInt($('#hmc_redeem_points').data('redeem-points'));
			  
			  scope.activateLayerText =  scope.activateLayerTextInit.replace("{0}", scope.redeemPoints);

			  if(scope.clubModel.currentlyActiveService.offerKey){
				  scope.activateOverwriteLayerText =  scope.activateOverwriteLayerTextInit.replace("{0}", scope.redeemPoints)
					.replace("{1}", scope.clubModel.currentlyActiveService.title);
			  }
			  
		  		self.setOfferMode(scope.offerMode); 		
		  	}
	    	
	    	//[PS] Personalized expire date management
	    	if(scope.offerPropositionIds[scope.offerKey] && scope.offerPropositionIds[scope.offerKey].personalizedExpireDate){
	    		var personalizedExpireDate = scope.offerPropositionIds[scope.offerKey].personalizedExpireDate;
	    		$(".valid time").text(self.getGentleDate(personalizedExpireDate));
	    	}else if(myOfferInRedeem && myOfferInRedeem.personalizedExpireDate) {
	    		$(".valid time").text(self.getGentleDate(myOfferInRedeem.personalizedExpireDate));
	    	}
	    	
	    	//Redeem call	- TOUCH mode  	
	  		if(!scope.offerKeyOnRedemptionNow){
	  			scope.redeemPoints = parseInt($('#hmc_redeem_points').data('redeem-points'));
		      if(scope.isUpToDate){		        
		        if(scope.redeemPoints>data.pointsBalance){
		        	scope.notEnoughPoints = true;
		        }
		        var isRedeemable = scope.redeemPoints==0 || !isNaN(scope.redeemPoints) && scope.redeemPoints <= data.pointsBalance;	        
		        self.enableOfferRedemption(!!isRedeemable);
		      }
		      self.doRedeemOfferCall(scope.clubModel.REDEEM_MODE.TOUCH);
		    }
	  		
	  		self.setOfferMode(scope.offerMode);
	  			  		
	    	setTimeout(function(){
		 			scope.$apply();
		 		},0); 	    
	    });
	  }

	  if(scope.isClubOfferPage || scope.isClubFullMemberPage){
	    
	    if(scope.isUpToDate){

	    	if(scope.isClubFullMemberPage){      
		      this.fetchAvailableOffers();      
		    }
		    scope.customerEanCodeUrl = data.customerBarcodeURL;
		    scope.customerLoyaltyId = data.customerLoyaltyId;

	      $('#hmc_points').text(data.pointsBalance);                                                   
	      var $mobilePoints = $('#hmclubeancodecardpoints');
	      var mobilePointsSuffix = $mobilePoints.data('points-suffix');
	      $mobilePoints.text(data.pointsBalance + " " + mobilePointsSuffix);

	      var $fullMemberClubPoints = $('#hmclubmypoints'),  
	      	fullMemberClubPointsSuffix = $fullMemberClubPoints.data('points-suffix');
	      $fullMemberClubPoints.text(data.pointsBalance + fullMemberClubPointsSuffix);
	    }
	    else
	    { 
	    	scope.isLosDown = true;
	    	$('#hmclubmypoints').text("--");	    	
	      $('#hmc_redeem_in_store').hide();
	      scope.$vouchersservicedown.find("*").show();
	    } 
	        	     
	    if(scope.offerKeyOnRedemptionNow){
	      if(scope.isClubOfferPage){                  
	        if(scope.offerKeyOnRedemptionNow == scope.offerKey){
	          this.startCountDown();
	        }else{
	          $("#hmc_redeem_in_store").hide();
	        }
	      }else{	//fullmember club page
	        this.startCountDown();
	      }       
	    }
	  }

	  setTimeout(function(){
 			scope.$apply();
 		},0);
	};
	
	this.loadBookingInfo = function(bookingId, venueCompanyId){
		
		var self = this;
		
		if(bookingId && venueCompanyId){
  		
  		scope.spinnerOnPersonalDetails = scope.spinnerOnBookingDetails = true;
  		
  		hmClubNetService.getBookingDetails({ bookingId: bookingId, venueCompanyId: venueCompanyId})
  		.then(function(response){
  			
  			scope.spinnerOnPersonalDetails = scope.spinnerOnBookingDetails = false;
  			
  			var booking = response.data;
  			scope.clubModel.loadedBooking = booking;
  			
  			scope.selectedVenueText = "";
  			scope.bookingId = booking.bookingId;
  			scope.venueCompanyId = booking.venueCompanyId;
  			scope.offerPropositionId = booking.offerPropositionId;
  			scope.whenDate = self.getGentleDate(booking.bookedDateTime);
  			scope.whenTime = self.getGentleTime(booking.bookedDateTime);
  			scope.duration = booking.duration;
  			scope.bookedDateTime = booking.bookedDateTime;
  			scope.selectedDateTimeText = scope.whenDate + " " + scope.whenTime;
  			scope.selectedDate = {
  					date: scope.whenDate
  			};
  			scope.selectedTimeSlot = {
  					time: scope.whenTime,
  					duration: scope.duration
  			};
  			scope.bookingLastCancellationDateTime = booking.lastCancellationDateTime;
  			
  			scope.whereStoreTitle = booking.address.address1;
  			scope.whereStreet =  booking.address.address2;
  			scope.whereCity = booking.address.address3;	
  			
  			scope.bookingStatus = booking.bookingStatus;
  			scope.clubModel.personalDetails.email = booking.email;
  			scope.clubModel.personalDetails.name = booking.firstName + " " + booking.lastName;
  			scope.clubModel.personalDetails.firstName = booking.firstName;
  			scope.clubModel.personalDetails.lastName = booking.lastName;
  			scope.clubModel.personalDetails.fullPhoneNumber = booking.phoneNumber;
  			if(booking.phoneNumber){
    			scope.clubModel.personalDetails.phonePrefix = booking.phoneNumber.substring(0,3).replace(/\s/g,'');
    			scope.clubModel.personalDetails.phoneNumber = booking.phoneNumber.substring(3).replace(/\s/g,'');
  			}  			
  			
  			var _bringAFriendVisible = scope.preferences.bringAFriendVisible;
  			scope.preferences = {
  				smsAlertRequired: booking.notificationSms,
  				mailAlertRequired: booking.notificationEmail  				  				
  			};
  			
  			if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_EVENT){  
  				
  				scope.preferences.bringAFriendChecked = booking.eventInfo.bringFriends==1 ? true : false;
  				scope.preferences.bringAFriendVisible = _bringAFriendVisible;
  				scope.preferences.bringAFriendName= booking.eventInfo.friendsNames[0];
  				
  				scope.selectedVenue = {
    					venueCompanyId: booking.venueCompanyId,
    					venueEventId: booking.eventInfo.venueEventId,
    					venueEventChainId: booking.eventInfo.venueEventChainId,  					
    					eventDateTime: booking.bookedDateTime,
    					duration: booking.duration
    			};
  				
  				scope.offerEditUrl = window.location.href;
  			}
  			else if(scope.offerType==scope.clubModel.OFFER_TYPE.CLUB_SERVICE){
  				
  				scope.offerEditUrl = window.location.href;
  			
  				//[PS] enrich already present selected store ( if present )
  				if(scope.selectedStore){
					scope.selectedStore.bookedDateTime= booking.bookedDateTime;
					scope.selectedStore.duration= booking.duration;
					scope.selectedStore.selected= true;
  				}
				
				//[PS] also for SA we have to check if user can modify event
				scope.eventLastRegistrationDateTime = booking.lastCancellationDateTime;
		                scope.isOfferAfterRegistrationDate = (function(lrdt){
		                       var lastRegistrationDate = new  Date(lrdt),
		                       now = new Date();
		                       if(lastRegistrationDate>now){
		                              return false;
		                       }
		                       return true;
		                })(scope.eventLastRegistrationDateTime);

				
				//[PS] now date time is editable and i need info from service
				scope.whenButton.removeAttr('disabled');	
				//[PS] here is not necessary cause is called by init stores info
				//self.loadOfferStoreDates();
  			}
  			  
  			scope.AmIBookedToThisOffer = true;
  			self.setOfferMode("view");
  			
  			self.setRecapBookingInfo();
  			self.setAddToCalendarUrls();

           scope.isBookEventButtonEnabled = self.checkIfValidForBooking();
  			
  			setTimeout(function(){
  	 			scope.$apply();
  	 		},0);
  		},
  		function(err){
  			self.manageBBIsDown();
  		});
  	}else{
  		//Booking Mode	    		
  		setOfferMode("booking");
        scope.isBookEventButtonEnabled = self.checkIfValidForBooking();
  	}
	}

	this.checkIfRedirectionIsNeeded = function(status){

	  var status = status || scope.userStatus;
	  
	  if(scope.hmRedirectPath){
      if(scope.clubModel.urlMapping[status]){
      	scope.clubPageUrl = scope.hmRedirectPath.replace('selector-placeholder', scope.clubModel.urlMapping[status]);	          
      }else{
      	scope.clubPageUrl = scope.hmRedirectPath.replace('selector-placeholder', 'guest');
      }                            	                 
    }

	  if(scope.clubModel.pagesMapping[status] != scope.pageType
		//[PS] the redirection check is disabled for guest page??
	  	//&& !scope.isClubGuest 
	  	&& !scope.isClubOfferPage 
	  	|| (status=="NON_LOGGED" && scope.isClubOfferPage))
	  {
	    if(scope.pageType 
	      && scope.clubModel.pagesMapping[status]!=scope.pageType
	      && !(scope.isClubOfferPage && scope.userStatus == 'FULL_MEMBER')){	      	      	
	      
	      document.location = scope.clubPageUrl; 
	    }
	  }
	};

	this.fetchAvailableOffers = function(callBackPromise) {

	  var self = this, 	    
	    $vouchersList,
	    offerKey;

	  if(scope.userStatus=='FULL_MEMBER'){

	    var getOfferPropositionsCallBack = function(data){        
	     
        if(data.length){   
        	
          data.sort(function(a,b){
            return a.rank - b.rank;
          });
          
          scope.offerPropositionIds = {};
          
          for(var i=0;i<data.length;++i){
          	var item = data[i];
          	scope.offerPropositionIds[item.offerKey] = {
            		offerPropositionId: item.offerPropositionId,
            		offerDiscountCodeStore: item.offerDiscountCodeStore,
            		personalizedExpireDate: item.personalizedExpireDate
            };
          }
          
      		//[PS] getting timestamp for calculate timeShift
		if(item.timestamp && !sessionStorage.getItem(scope.timeShiftParam)){
			sessionStorage.setItem(scope.timeShiftParam, self.getGentleTimeShift(item.timestamp));
		}
          
//          data.forEach(function(item){        
//            scope.offerPropositionIds[item.offerKey] = {
//            		offerPropositionId: item.offerPropositionId,
//            		offerDiscountCodeStore: item.offerDiscountCodeStore
//            };            
//          });

          try{
          	localStorage.setItem(scope.offerPropositionIdsName, JSON.stringify(scope.offerPropositionIds));
          }catch(err){} 
        
          if(scope.isClubFullMemberPage){                
            var arrayOfOffers = [],
            	eventIds = [];
            for(var i=0;i<data.length;++i){
            	var item = data[i],
            	$elementToAppend = $($vouchersList.find("#hmv_" + item.offerKey));
            	
            	//[PS] personlized expire date in voucher list management
            	if(item.personalizedExpireDate){
            		var $validUntilPersonal = $elementToAppend.find('.valid time');
            		$validUntilPersonal.text(self.getGentleDate(item.personalizedExpireDate));
  				}
	            	
              if(item.offerType==scope.clubModel.OFFER_TYPE.CLUB_EVENT){
              	if($elementToAppend.data("club-event-chain-id")){
              		eventIds.push($elementToAppend.data("club-event-chain-id"));
              	}
            	}
              if($elementToAppend.length){
              	arrayOfOffers.push($elementToAppend);
              }
            }
            
//            data.forEach(function(item){              
//              var $elementToAppend = $($vouchersList.find("#hmv_" + item.offerKey));
//              if(item.offerType==scope.clubModel.OFFER_TYPE.CLUB_EVENT){
//              	if($elementToAppend.data("club-event-chain-id")){
//              		eventIds.push($elementToAppend.data("club-event-chain-id"));
//              	}
//            	}
//              if($elementToAppend.length){
//              	arrayOfOffers.push($elementToAppend);
//              }
//            });      
            
            if(eventIds.length){
            	var eventsPayload = { clubEventChainIds: eventIds.join() };

	            hmClubNetService.getEvents(eventsPayload).then(function(response){
		            	
            	var events = response.data;
				if(events && !events.error){	
							
					for(var i=0;i<arrayOfOffers.length;++i){
						//last registration date
						var $item = arrayOfOffers[i];
						
						if($item.data("club-event-chain-id")){
							
							var $validUntil = $item.find('.valid time');
							var eventChainId = $item.data("club-event-chain-id");
	              		
							for(var j=0;j<events.length;++j){
		              			var event = events[j];
		              			
		              			if(event.clubEventChainId==eventChainId){
		              				
		              				if($validUntil.length){
										$validUntil.text(self.getGentleDate(event.lastRegistrationDateTime));
									}
			              			
		              				if(!event.registrable){		              						              				
		              					$item.find('.notPossibleToBookMessage').show();
		              				}else if(event.venues && event.venues.length==1){
		              					var venue = event.venues[0];		              					
		              					if(!event.registrable){
		              						$item.find('.registrationClosedMessage').show();
		              					}else{
		              						if(venue.seatsLeft==0){
			              						$item.find('.fullyBookedMessage').show(); 
			              						scope.isRedeemButtonHidden=true;
			              					}else if(venue.fewSeatsAvailable){
			              						$item.find('.fewSeatsLeftMessage').show();
			              					}		              								              					
		              					}		              							              				
		              				}
		              			}	
							}
						}
					}
				}else{
					console.log(events.error.reasonCode);
					self.manageBBIsDown(arrayOfOffers);
				}		
				},
			    function(err) {
						self.manageBBIsDown(arrayOfOffers);
			    });
            }
            
            ///
            var displayVouchers = function(){
            	
            	$vouchersList.empty();
            	
            	//since IE8 does not support forEach on array,
            	//we must provid e a cycle that create a closure for the event handler that will apply to the cta
            	//so we use an instant function             	
            	for(var i=0;i<arrayOfOffers.length;++i){
            		(function($item){            			
              		$vouchersList.append($item);            
  	              var $cta = $item.find('.cta.button');
  	              if ($cta.length == 1) {
  	                var href = $cta.attr('href');
  	                $item.show();
  	                $item.addClass('single-cta');
  	                $item.on('click', function(){
  	                  window.location = href;
  	                  return false;
  	                });
  	              }
            		})(arrayOfOffers[i]);            		
            	}            		           
	            scope.$vouchers.find("*:not('.state')").show();
	            if(hm.responsiveImages) {
	              hm.responsiveImages.update(scope.$vouchers);
	            }
            };
						
            hmClubNetService.getBookings().then(function(response){
            	
            	//[PS] to be sure that local storage is aligned for OSP, we remove localStorage key if presents
            	localStorage.removeItem(scope.alreadyActiveOSPKey);
            	
            	var elements = response.data;
            	
					if(elements && elements.length){
								
						var vouchersInfo = {};
						$vouchersList.find('article[id^="hmv_"]').each(function(){
							var $item = $(this);
							vouchersInfo[$item.attr('id').replace('hmv_','')]={
								vignette: $item.find('.heading').text(),
								title: $item.find('.hmv_title').text(),
								url: $item.find('.cta-overlay').attr('href')
							};																		
						});
						
						var tempVouchersArray = [];
						var tempActiveOffersArray = [];
						
						for(var i=0;i<elements.length;++i){
							var element = elements[i];
							
							if(element.error){
								//[PS] Make error management based on BOOKINGS_SERVICE_UNAVAILABLE or ACTIVE_OFFERS_SERVICE_UNAVAILABLE
								continue;
							}
							
							var voucher = vouchersInfo[element.offerKey];																		
							
							if(voucher){									
								var date = "";
								var voucherToPush = {
										vignette: voucher.vignette,
										title: voucher.title,	
									};
								
								if(!element.isActiveOffer){
									date = (element.bookingDateTime) ? self.getGentleDate(element.bookingDateTime) : "";
									voucherToPush.date = date;
									voucherToPush.url= voucher.url + "#?bid=" + element.bookingId +"&vcid=" + element.venueCompanyId
									tempVouchersArray.push(voucherToPush);
								} else{
									date = (element.onlineServices && element.onlineServices[0]) ? self.getGentleDate(element.onlineServices[0].endDate) : "";
									voucherToPush.date = date;
									voucherToPush.url= voucher.url;
									tempActiveOffersArray.push(voucherToPush);	
									//[PS] this variable serve to check if user can have another service package
									localStorage.setItem(scope.alreadyActiveOSPKey,JSON.stringify({"offerKey":element.offerKey, "endDate":date, "title":voucher.title}));
									scope.clubModel.currentlyActiveService = {"offerKey":element.offerKey, "endDate":date, "title":voucher.title};
								}
							}
							else{
								console.log("No voucher corrensponding to: " + element.offerKey + " can be found.");										
							}
						}
						
						tempVouchersArray.sort(function(a,b){
							if(a.date < b.date) return -1;
							    if(a.date > b.date) return 1;
							    return 0;
						});
						
						scope.upcomingBookings = tempVouchersArray;
						
						tempActiveOffersArray.sort(function(a,b){
							if(a.date < b.date) return -1;
							    if(a.date > b.date) return 1;
							    return 0;
						});
						
						scope.activeOffers = tempActiveOffersArray;
							
						setTimeout(function(){
	    	 			scope.$apply();
	    	 		},0);
						displayVouchers();
					}else{
						displayVouchers();
					}
				},
		    function(err) {
					self.manageBBIsDown(arrayOfOffers);
		    });	//end promise            
          }
        }
        else if(scope.$vouchersnooffers){
        	
        	console.log(scope.$vouchersnooffers);
        	scope.$vouchersnooffers.find("*").show();                
        }                                    
	      
	      //Just after processin response, if any callback is defined it should be called
	      if(typeof callBackPromise == 'function'){        
	        callBackPromise();  
	      }    
	    };

	    if(scope.isClubFullMemberPage)
	    {	    	
	      $vouchersList = scope.$vouchers.children('.three.modules');      
	    }
	        
	    var payload = {        
	      businessPartnerId: scope.businessPartnerId,
	      offerSpace: scope.offersSpace,
	      maximumNumberOfOffers: scope.memberOffers
	    };

	    hmClubNetService.getOffersProposition(payload).then(function(response){
	    	if(response.data){
		 			getOfferPropositionsCallBack(response.data);
			  }
			},
			function(err){
				$vouchersList.empty();
  			if(scope.$vouchersservicedown){
          scope.$vouchersservicedown.find("*").show();   //TODO: from porting jquery, change to ng-show           
        }
  		});
	  }
	  else {
	    if(scope.isClubFullMemberPage){
	      document.location = document.location.pathname.replace('fullmember','guest');
	    }
	  }
	};



	this.doRedeemOfferCall = function(propositionStatusId, doRedeemOfferCallBack) {

	  var self = this;
	 
	  if(scope.offerPropositionId){
    	
    	if(scope.offerPropositionIds) {
	      payload = {        
	        customerLoyaltyId: scope.customerLoyaltyId,
	        pointsImpactValue: scope.redeemPoints,
	        pointsImpactFlag: 0,
	        offerType: scope.offerType,
	        OfferKey: scope.offerKey,
	        offerPropositionId: scope.offerPropositionId,
	        propositionStatusId: propositionStatusId 
	      };

		    hmClubNetService.redeemOffer(payload).then(function(data, err){
		    	if(doRedeemOfferCallBack){
			    	if(err){
			    		doRedeemOfferCallBack(propositionStatusId,err);
					}else{
						doRedeemOfferCallBack(propositionStatusId);
					}	
		    	}
		    	setTimeout(function(){
    	 			scope.$apply();
    	 		},0); 
			});
		  }
    }else{
    	console.logp("WARN: offerPropositionId is undefined, no redeemCall on this Offer can be performed.");
    }
	};

	this.redeemOffer = function() {
		try{
	    var $articleDetails = $('article.redeeming-details');
	    localStorage.setItem('hmc_stored_currently_redeem_offer_link', JSON.stringify(window.location.href.split('#')[0]));
	    localStorage.setItem('hmc_stored_currently_redeem_offer_id', JSON.stringify(scope.offerKey));
	    //[PS] i have to save in local storage the json object representing offer formatted like we format the los response for offerPropositionIds
	    localStorage.setItem('hmc_stored_currently_redeem_offer_proposition_id', JSON.stringify(scope.offerPropositionIds[scope.offerKey]));
	  }catch(err){}
	  
	  	var doRedeemOfferCallBack = function(propositionStatusId, err){  
		    if(err || !scope.isUpToDate){  
			    if (err && err.status == 403) {
		        $('#hmc_connectivity_issue').show();
		      }
		    }else{         
		      if(propositionStatusId && propositionStatusId===scope.clubModel.REDEEM_MODE.ACTUAL_REDEEM){  //redeem
		        self.startCountDown();	        
		        localStorage.removeItem(scope.offerPropositionIdsName);	        
		      }
		    }    
		  };

	  this.doRedeemOfferCall(scope.clubModel.REDEEM_MODE.ACTUAL_REDEEM,doRedeemOfferCallBack);
	  this.scrollTop();
	  
	  return false;
	};
	
	this.redeemServicePackage = function() {
		
	  	var doRedeemOfferCallBack = function(propositionStatusId,err){  
		    if(err || !scope.isUpToDate){  
			    if (err && err.status == 403) {
		        scope.errorRedeemingServicePackage = true;
		      }
		    }else{         
		      if(propositionStatusId && propositionStatusId===scope.clubModel.REDEEM_MODE.ACTUAL_REDEEM){  //redeem
		        localStorage.removeItem(scope.offerPropositionIdsName);
		        
		        //We have to calculate the duration
		        
		        var validUntilAfterRedeem = moment().add(scope.ospDuration,'days').format("YYYY-MM-DD");
		        if(scope.servicePackageValidUntilText && scope.servicePackageValidUntilText.length){
		        	scope.servicePackageValidUntilText = scope.servicePackageValidUntilText.replace("{0}", validUntilAfterRedeem);
		        } 
		        if(scope.lastRegistrationDateText && scope.lastRegistrationDateText.length){
  	 				scope.lastRegistrationDateText = scope.lastRegistrationDateText.replace('{0}', validUntilAfterRedeem);
  	 			}
		        $(".valid").show();
		        localStorage.setItem(scope.alreadyActiveOSPKey, JSON.stringify({"offerKey":scope.offerKey, "endDate" : validUntilAfterRedeem}));
		        scope.clubModel.currentlyActiveService = {"offerKey":scope.offerKey, "endDate" : validUntilAfterRedeem}
		        self.setOfferMode("confirmation");
		        scope.showServiceValidUntil=true;
		        scope.showServiceDuration=false;
		      }
		    }    
		  };

	  this.doRedeemOfferCall(scope.clubModel.REDEEM_MODE.ACTUAL_REDEEM, doRedeemOfferCallBack);
	  this.scrollTop();
	  
	  return false;
	};

	this.startCountDown = function() {

	  var storedEndTime = JSON.parse(localStorage.getItem('hmc_stored_voucher_expire_redeem_time'));
	  
	  scope.currentlyRedeemOfferLink = JSON.parse(localStorage.getItem('hmc_stored_currently_redeem_offer_link'));
	  scope.isVoucherCountDownVisible = true; 	  	
	  scope.isRedeemButtonHidden = true;
	  
	  setTimeout(function(){
 			scope.$apply();
 		},0); 
	  
	  var timer = $('#hmc_redeem_voucher_timer'),
	    durationMinutes = scope.countDownDurationMinutes,	//timer.data('timer-expired'),
	    endTime = Date.parse(new Date()) + durationMinutes * 6E4;

	  if(storedEndTime){
	    endTime = storedEndTime;    
	  }else{
	    try{
	      localStorage.setItem('hmc_stored_voucher_expire_redeem_time', JSON.stringify(endTime));
	    }catch(err){}   
	  }

	  scope.counterIterval = setInterval(function(){
	    var t = endTime - Date.parse(new Date()),
	      seconds = Math.floor( (t/1000) % 60),
	      minutes = Math.floor( (t/1000/60));
	    if(minutes<=0 && seconds<0){
	      clearInterval(scope.counterIterval);
	      localStorage.removeItem('hmc_stored_currently_redeem_offer_id');
	      localStorage.removeItem('hmc_stored_currently_redeem_offer_link');
	      localStorage.removeItem('hmc_stored_voucher_expire_redeem_time');
	      //[PS] remove the new object put to manange ean
	      localStorage.removeItem('hmc_stored_currently_redeem_offer_proposition_id');
	      
	      scope.isVoucherCountDownVisible = false;
	      scope.isVoucherCountDownExpiredMessageVisible = true;
	      scope.$apply();
	    }else{      
	      if(minutes<10){ minutes = '0' + minutes; }      
	      if(seconds<10){ seconds = '0' + seconds; }       
	      timer.text(minutes + ':' + seconds);
	    }
	  }, 1000);
	};
	
	this.enableOfferRedemption = function(enable) {  

	  var $pointsWarning = $('#hmc_not_enough_points'),
	    $redeemMessage = $('#hmc_redeem_offer_message'),
	    $book = $('#hmc_book_voucher_button');

	  if(enable) {
	    $pointsWarning.hide();
	    $redeemMessage.show();        
	    $book.show();
	    scope.isRedeemButtonHidden = false;
	    scope.showActivateButton = true;
	  }
	  else{        
	    $redeemMessage.hide();    
	    $pointsWarning.show();
	    $book.hide();
	    scope.isRedeemButtonHidden = true;
	    scope.showActivateButton = false;
	  }  
	};

	this.initModalAccordions = function(){
		var priv = {
          // Selectors will be queried on init and cached in priv.$elements
          elementSelectors: {
              accordionElement: '.accordion-triangle',
              itemElemt: '.item',
              listElement: '.multi-selectable-sub-items',
          }
      },
      activeClass = 'active',
      collapsedClass = 'collapse';

	  priv.init = function(){
	      priv.setupEventListeners();
	  };
	  priv.setupEventListeners = function() {
	      $(document).on('click', priv.elementSelectors.accordionElement, priv.accordion);
	  };

	  priv.accordion = function(e) {
	      $this = $(e.currentTarget);
	      $target = $this
	                  .parent(priv.elementSelectors.itemElement)
	                  .next(priv.elementSelectors.listElement);

	      if($target.hasClass(activeClass)){
	          $target
	              .removeClass(activeClass)
	              .slideUp(); 
	          $this
	              .removeClass(collapsedClass);
	      } else {
	          $(priv.elementSelectors.listElement)
	              .removeClass('active')
	              .slideUp();
	          $target
	              .addClass(activeClass)
	              .slideDown();
	          $(priv.elementSelectors.accordionElement)
	              .removeClass('collapse');
	          $this
	              .addClass(collapsedClass);
	      }
	      return false;
	  }
	  priv.init();
	};

	this.filterOfferCitiesFromOfferStores = function(m){
		m.offerCities = [];
		var tempCityKeys = [];
		
		for(var i =0; i<m.filteredOfferStores.length;++i){
			var store = m.filteredOfferStores[i];
	
			if(tempCityKeys.indexOf(store.cityKey)<0){
				tempCityKeys.push(store.cityKey);
				var newCity = { key: store.cityKey, name: store.cityName };
				m.offerCities.push(newCity);
				if(scope.selectedStore && scope.selectedStore.cityKey == store.cityKey){
					scope.selectedCity = newCity;
					scope.selectCityChange();
				}
			}
		}
	};

	this.initStoresInfo = function(){

		scope.spinnerOnConceptsButton = true;
		
		var self = this;
		hmClubNetService.getStoresInfo().then(function(response){

			var storeResponse = response.data;
	 		if(storeResponse){
	 			$(storeResponse).find('storeComplete').each(function(){

			    var $store = $(this),
			      cityName = $store.find('> city').text(),
			      cityKey = cityName.replace(" ", "_").toLowerCase(),
			      storeId = $store.find('storeId').text(),
			      storeName = $store.find('> name').text(),
			      storePhone = $store.find('> phone').text();

			    ////////////////////////////////////////////////////////////////////////////////////    
			    if(!scope.clubModel.storesByCity[cityKey]){
			      scope.clubModel.storesByCity[cityKey] = {
			        cityName: cityName,
			        stores: []
			      };
			      scope.clubModel.cities.push({ key: cityKey, name: cityName });
			    }    
			    scope.clubModel.storesByCity[cityKey].stores.push({
			      id: storeId,
			      name: storeName
			    });

			    ////////////////////////////////////////////////////////////////////////////////////
			    var address = '';
			    $store.find('address addressLine').each(function(){
			      if(address.length > 0) {
			        address += ' ';
			      }
			      address += $(this).text();
			    });
			    var store = {
			      id: storeId,
			      name: storeName,      
			      address: address,
			      telephone: storePhone,
			      cityKey: cityKey,
			      cityName: cityName,
			      departments: []  
			    };

			    $store.find('departmentWithConcepts').each(function(){
			      var $department = $(this),
			        department = {};        

			      department.id = $department.find('> departmentId').text();
			      department.name = $department.find('> name').text();
			      department.concepts = [];

			      var departmentKey = department.name.replace(/\W+/g, "_").toLowerCase();			      
			      department.departmentKey = departmentKey;

			      if(!scope.clubModel.departments[departmentKey]){
			        scope.clubModel.departments[departmentKey] = {			        	
			          id: department.id,
			          name: department.name,
			          concepts: {},
			          supportedByStoresIds: []
			        };
			      }
			      scope.clubModel.departments[departmentKey].supportedByStoresIds.push(storeId);
			      
			      $department.find('concepts concept').each(function(){
			        var $concept = $(this)
			          conceptName = $concept.find('> name').text(),
			          conceptId = $concept.find('> conceptId').text();
			          //conceptKey = conceptName.replace(/\W+/g, "_").toLowerCase();

			        var concept = {
			          departmentId: department.id,
			          conceptId: conceptId,
			          name: conceptName
			        };
			        department.concepts.push(concept);

			        ////////////////////////////////////////////////////////////////////////////////////
			        if(!scope.clubModel.departments[departmentKey].concepts[conceptId]){
			          var cpt = jQuery.extend(true, {}, concept);
			          cpt.supportedByStoresIds = [];
			          cpt.supportedByStoresIds.push(storeId);
			          scope.clubModel.departments[departmentKey].concepts[conceptId] = cpt;
			        }else{
			          scope.clubModel.departments[departmentKey].concepts[conceptId].supportedByStoresIds.push(storeId);
			        }
			      });
			      store.departments.push(department);			      
			    });			    
			    scope.clubModel.stores[storeId] = store;
			  });
			}
			 console.log(JSON.stringify(scope.clubModel.storesByCity));
			 console.log(scope.clubModel.storesByCity);
			 console.log(scope.clubModel.stores);
			 console.log(scope.clubModel.cities);
			 console.log(scope.clubModel.departments);		

		  hmClubNetService.getServices({ clubServiceId: scope.clubServiceId })
		  .then(function(response){
		  	
		  	var services = response.data;
		  	
	 			if(services.venueServiceInfo){
	 				
	 				scope.serviceBookingMinimumDaysInAdvance = services.minimumDaysInAdvance;
	 				for(var i=0; i<services.venueServiceInfo.length; ++i){
	 					var store = scope.clubModel.stores[services.venueServiceInfo[i].storeCode];
	 					if(store){
		 					store.venueCompanyId=services.venueServiceInfo[i].venueCompanyId;
		 					store.venueServiceId=services.venueServiceInfo[i].venueServiceId;
		 					//[PS] to autoselect store and enrich with info from bookingINf ( if presents)
		 					if(scope.clubModel.vcid == store.venueCompanyId){
		 						store.selected=true;
		 						if(scope.duration && scope.bookedDateTime){
			 						store.bookedDateTime = scope.bookedDateTime;
			 						store.duration = scope.duration;
		 						}
		 						scope.selectedStore = store;
		 						scope.venueCompanyId = store.venueCompanyId;
		 						scope.isSaveSelectLocationButtonDisabled = false;
		 						self.loadOfferStoreDates();
//		 						if (scope.selectedStore && scope.selectedStore.selected){
//		 							scope.selectedCityText = "";
//		 							scope.whenButton.removeAttr('disabled');				
//		 							self.loadOfferStoreDates();
//		 						}	else{
//		 							scope.cleanSelectedStore();
//		 							scope.whenButton.attr('disabled','');
//		 						}
		 					}
		 					scope.clubModel.offerStores.push(store);
	
		 					for(var j=0; j<store.departments.length; ++j){
		 						var dep = store.departments[j];
	
		 						if(!scope.clubModel.offerDepartments[dep.departmentKey]) {
		 							
		 							scope.clubModel.offerDepartments[dep.departmentKey] = {
		 								id: dep.id,
		 								name: dep.name,
		 								concepts: {}
		 							};
		 							for(var k=0; k<dep.concepts.length; ++k){
		 								var concept = dep.concepts[k];	 									 								
		 								if(!scope.clubModel.offerDepartments[dep.departmentKey].concepts[concept.conceptId]) {
		 									scope.clubModel.offerDepartments[dep.departmentKey].concepts[concept.conceptId] = {
		 										id: concept.conceptId,
		 										name: concept.name,
		 										departmentId: dep.id,
		 										departmentKey: dep.departmentKey
		 									}
		 								}
		 							}
		 						}
		 					}
		 				}
					}
					scope.clubModel.filteredOfferStores = scope.clubModel.offerStores;
					self.filterOfferCitiesFromOfferStores(scope.clubModel);
					setTimeout(function(){
    	 			scope.$apply();
    	 		},0); 
	 			} 			
	 			scope.interestedInTextInitial = scope.interestedInText;
	 			scope.selectedCityTextInitial = scope.selectedCityText;
	 			scope.selectedDateTimeTextInitial = scope.selectedDateTimeText;	 			
			});
		  scope.spinnerOnConceptsButton = false;
 		},
 		function(err){
			scope.isBBDown = true;			
			scope.spinnerOnConceptsButton = false;
			self.manageBBIsDown();
		});
	};

	this.loadOfferStoreDates = function(){

		var self = this,
		 payload={ venueServiceId: scope.selectedStore.venueServiceId,
									venueCompanyId: scope.selectedStore.venueCompanyId,
									//startDate: moment().format('YYYY-MM') + "-01",
									startDate: moment().add(scope.serviceBookingMinimumDaysInAdvance, 'days').format('YYYY-MM-DD'),									
									endDate: scope.offerEndDate
								};
		
		hmClubNetService.getServiceStoreDates(payload)
		.then(function(response){
			var storeDates = response.data;
			if(storeDates && storeDates.length){				
				hmBookingCalendar.eventArray = [];
				for(var i=0;i<storeDates.length;++i){
					if(storeDates[i].available){
						hmBookingCalendar.eventArray.push({date: storeDates[i].date, id: i });
					}
				}
				hmBookingCalendar.scope = scope;
				hmBookingCalendar.refreshCalendar();				
			}else{
				scope.isNoDatesAvailableMessageVisible = true;
			}
			setTimeout(function(){
	 			scope.$apply();
	 		},0);
		},
		function(err){
			scope.isBBDown = true;
		});
	};

