}else{return a.parseUri(c).search
}};
b.getPathAndQuery=function(){var c=b.getUrl();
return c.split(/\#/)[0]
};
b.addParamToUrl=function(c,f,d){var e=c.split("#");
if(!f.length){return c
}if(e[0].indexOf("?")===-1){e[0]+="?"
}else{e[0]+="&"
}if(typeof d!=="undefined"){f=[f,d].join("=")
}e[0]+=f;
