}return c
};
a.validateDOB=function(h){var e={isValid:true},g=h.val(),d=h.data("min-age"),f=g.split("-"),c=new Date(parseInt(f[0]),parseInt(f[1])-1,parseInt(f[2]));
if(!g.length){return e
}if(f[0].length!==4){e.isValid=false;
e.errorMessage=h.data("validation-minlength-text")
}else{if(!moment(g,"YYYY-MM-DD").isValid()){e.isValid=false;
e.errorMessage=h.data("format-text")
}else{if(!isNaN(c.valueOf())){e.isValid=a.meetsMinimumAge(c,d);
e.errorMessage=h.data("min-age-text").replace("XX",d)
}}}return e
};
a.meetsMinimumAge=function(d,c){var e=new Date(d.getFullYear()+c,d.getMonth(),d.getDate());
return(e<=new Date())
};
