c.removeClass("hidden");
if(c.css("position")==="absolute"){e=c.prev();
g=parseInt(e.css("padding-bottom"));
d=parseInt(c.outerHeight());
e.css("padding-bottom",g+d)
}hm.textTools.truncateByHeight(h,f)
};
a.showModal=function(f){var d=a.findModal(f),e=b.$elements.fixedHeightElement.eq(f).closest("section").find(a.elementSelectors.modalTitle).text()||"",c=d.content;
c.find("img").remove();
