}this.rimd=new Rimd({closestAbove:true,reloadOnResize:true,className:a,widths:[304,320,375,440,530,640,800,960,1280],path:hm.options.rimd.service||"{path}",pathOverride:hm.options.rimd.serviceOverride||{},lazyload:false,dubbleSizeRetina:true,retinaQuality:80,retinaMultiplyer:1.5});
this.setClassName(a);
this.eventListener()
};
